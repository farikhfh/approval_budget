	<VBox class="sapUiSmallMargin">
		<f:Form id="FormDisplay3" editable="false">
			<f:title>
				<core:Title text="Indikasi Perubahan PFCk"/>
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
						<f:FormElement label="RAP Awal HP 0%">
							<f:fields>
								<Text text="{Pfc0percentage} %" id="pfc0percentage"/>
								<Text text="{Pfc0}" id="pfc0"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="PFC Sebelum Update">
							<f:fields>
								<Text text="{Pfc} %" id="pfc2"/>
								<Text text="{OriTotalAllReal}" id="nilaipfc"/>
							</f:fields>
						</f:FormElement>						
						<f:FormElement label="PFCK">
							<f:fields>
								<Text text="{Pfck} %" id="pfck2"/>
								<Text text="{OriTotalAll}" id="nilaipfck"/>
							</f:fields>
						</f:FormElement>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormDisplay2" editable="false">
			<f:title>
				<core:Title text="Deviasi Budget"/>
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
						<f:FormElement label="Budget Original">
							<f:fields>
								<Text text="{OriBalanced}" id="totrelawalREAL"/>
							</f:fields>
						</f:FormElement>						
						<f:FormElement label="Budget Release">
							<f:fields>
								<Text text="{RelBalanced}" id="totrelawalPLAN"/>
							</f:fields>
						</f:FormElement>
				</f:FormContainer>
			</f:formContainers>
		<f:customData>
		    <core:CustomData key="w5g.dt.editable" value="false" id="__data2"/>
		</f:customData>
		</f:Form>
	</VBox>
</core:FragmentDefinition>